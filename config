ngx_addon_name=ngx_stream_parse_server_name_module

ECHO_SRCS="                                                                 \
        $ngx_addon_dir/src/ngx_stream_parse_server_name_module.c            \
        $ngx_addon_dir/src/submodules.c            \
        "

ECHO_DEPS="                                                                 \
        $ngx_addon_dir/src/ngx_stream_parse_server_name_module.h            \
        "

if [ -n "$ngx_module_link" ]; then
    ngx_module_type=STREAM
    ngx_module_name=$ngx_addon_name
    ngx_module_incs=
    ngx_module_deps="$ECHO_DEPS"
    ngx_module_srcs="$ECHO_SRCS"
    ngx_module_libs=

    . auto/module
else
    NGX_STREAM_MODULE="$NGX_STREAM_MODULE $ngx_addon_name"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ECHO_SRCS"
    NGX_ADDON_DEPS="$NGX_ADDON_DEPS $ECHO_DEPS"
fi
